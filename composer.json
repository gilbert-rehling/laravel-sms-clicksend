{
    "name": "gilbert-rehling/laravel-sms-clicksend-upgrade",
    "description": "ClickSend Notifications channel for Laravel 5.6 & 6.* - forked from vladski/laravel-sms-clicksend",
    "keywords": ["laravel", "notifications", "clicksend", "sms"],
    "homepage": "https://github.com/gilbert-rehling/laravel-sms-clicksend-5.6-6.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Gilbert Rehling",
            "role": "Developer",
            "email": "gilbert@gilbert-rehling.com",
            "homepage": "https://github.com/gilbert-rehling"
        },
        {
            "name": "Vladimir Kundrat",
            "role": "Developer",
            "email": "vlado@12345.com.au",
            "homepage": "https://github.com/vladski"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "clicksend/clicksend-php": "5.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2|8.0",
        "mockery/mockery": "^0.9.5|1.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\ClickSend\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\ClickSend\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
